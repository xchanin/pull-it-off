{
	"name": "@pio/pull-it-off",
	"version": "0.0.1",
	"scripts": {
		"build:all": "npm run build:common && npm run build:demo && npm run build:lcu",
		"build:common": "ng build common",
		"build:demo": "ng build demo --prod",
		"build:lcu": "ng build lcu --prod --single-bundle && npm run pack",
		"demo": "npm run build:common && npm run start:demo",
		"demo:lcu": "npm run build:common && npm run build:lcu && npm run start:demo",
		"deploy": "npm version patch && npm run deploy:all",
		"deploy:all": "npm run deploy:common && npm run deploy:lcu && npm run deploy:demo",
		"deploy:common": "npm version patch --prefix projects/common && npm run build:common && npm publish ./dist/common --access public",
		"deploy:demo": "npm version patch --prefix projects/demo && npm run build:demo && npm publish ./dist/demo --access public",
		"deploy:lcu": "npm version patch --prefix projects/lcu && npm run build:lcu && npm publish ./dist/lcu --access public",
		"ng": "ng",
		"pack": "npm run pack:lcu",
		"pack:join": "concat-glob-cli -f \"dist/lcu/wc/lcu.*.js\" -o dist/lcu/wc/pull-it-off.lcu.js",
		"pack:lcu": "rimraf dist/lcu/wc && mkdirp dist/lcu/wc && npm run pack:main && npm run pack:pollyfills && npm run pack:join",
		"pack:main": "concat-glob-cli -f \"dist/lcu/main-es2015.*.js\" -o dist/lcu/wc/lcu.startup.js",
		"pack:pollyfills": "concat-glob-cli -f \"dist/lcu/scripts.*.js\" -o dist/lcu/wc/lcu.pollyfills.js",
		"start": "ng serve demo --port=4293",
		"build": "npm run build:common && npm run build:lcu",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e"
	},
	"private": false,
	"dependencies": {
		"@angular/animations": "^10.2.1",
		"@angular/cdk": "^10.2.7",
		"@angular/common": "^10.2.1",
		"@angular/compiler": "^10.2.1",
		"@angular/core": "^10.2.1",
		"@angular/elements": "^10.2.1",
		"@angular/fire": "^6.1.4",
		"@angular/flex-layout": "^11.0.0-beta.33",
		"@angular/forms": "^10.2.1",
		"@angular/material": "^10.2.7",
		"@angular/platform-browser": "^10.2.1",
		"@angular/platform-browser-dynamic": "^10.2.1",
		"@angular/router": "^10.2.1",
		"@aspnet/signalr": "^1.1.4",
		"@lcu/common": "^1.49.391",
		"@lowcodeunit-devkit/ng-lcu": "^1.24.148",
		"@types/highlight.js": "^9.12.3",
		"@types/tinycolor2": "^1.4.2",
		"@vime/angular": "^5.0.19",
		"@webcomponents/custom-elements": "^1.4.3",
		"@webcomponents/webcomponentsjs": "^2.5.0",
		"copy": "^0.3.2",
		"firebase": "^8.2.1",
		"hammerjs": "^2.0.8",
		"highlight.js": "^9.18.1",
		"ngx-build-plus": "^10.1.1",
		"ngx-color-picker": "^10.1.0",
		"rxjs": "^6.6.3",
		"sass.js": "^0.11.1",
		"terser": "^5.3.8",
		"tinycolor2": "^1.4.1",
		"tslib": "^2.0.3",
		"zone.js": "~0.11.2"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^0.1002.0",
		"@angular-devkit/build-ng-packagr": "~0.1002.0",
		"@angular/cli": "~10.2.0",
		"@angular/compiler-cli": "^10.2.1",
		"@angular/language-service": "^10.2.1",
		"@lcu/cli": "^1.7.97",
		"@lowcodeunit/lcu-documentation-common": "^1.5.34",
		"@types/jasmine": "~3.6.0",
		"@types/jasminewd2": "~2.0.8",
		"@types/node": "^14.14.6",
		"@types/tinycolor2": "^1.4.1",
		"codelyzer": "^6.0.1",
		"concat-glob-cli": "^0.1.0",
		"cpx": "^1.5.0",
		"jasmine-core": "~3.6.0",
		"jasmine-spec-reporter": "~6.0.0",
		"karma": "~5.2.3",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "~3.0.3",
		"karma-jasmine": "~4.0.1",
		"karma-jasmine-html-reporter": "^1.5.4",
		"mkdirp": "^1.0.4",
		"ng-momentum": "0.0.24",
		"ng-packagr": "^10.1.2",
		"protractor": "^7.0.0",
		"rimraf": "^3.0.2",
		"scss-to-json": "^2.0.0",
		"ts-node": "~9.0.0",
		"tsickle": "^0.39.1",
		"tslint": "~6.1.3",
		"typed-scss-modules": "^3.3.0",
		"typescript": "~4.0.5"
	}
}
